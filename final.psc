funcion nivel1
	Definir vidaHP Como Entero
	Definir vidaQuirrel Como Entero
	Definir dañoQuirrel Como Entero
	Definir opcionAtaque Como Entero
	definir opc1 Como Entero
	Definir juegoActivo Como Logico
	
	// Inicializar variables
	vidaHP = 640
	vidaQuirrel = 800
	juegoActivo = Verdadero
	dañoQuirrel = azar(192) 
	// Definir el vector con diferentes opciones de ataque
	Dimension ataques[160]
	ataques[1] = 160 
	ataques[2] = 80  
	ataques[3] = 40  
	
	cargan1()
	carga()
	
	Escribir "Luego de pasar por todas las trampas colocadas en el pasillo prohibido te encuentras frente al espejo de oesed"
	Escribir "el profesor de defensa contra las artes oscuras Quirinus Quirrel"
	Esperar Tecla
	Limpiar Pantalla
	
	Mientras juegoActivo Hacer
		// El villano ataca con un daño al azar 
		Escribir "Quirrel:",vidaQuirrel
		quirrelataca()
		
		Escribir "Harry Potter:",vidaHP
		SI dañoQuirrel>=90 Entonces
			vidaHP = vidaHP - dañoQuirrel+48//defensa natural del jugador
		SiNo
			vidaHP = vidaHP - dañoQuirrel
		FinSi
		Escribir "@------------------------------------------------------------@"
		Escribir "|Quirrel te ataca y causa ", dañoQuirrel, " de daño.                          |"
		Escribir "|Tu vida queda en:", vidaHP "                                           |"
		Escribir "|                                                            |"
		Escribir "@------------------------------------------------------------@"
		
		Esperar Tecla
		Limpiar Pantalla
		Si vidaHP <= 0 Entonces
			Escribir "has sido derrotado."
			juegoActivo = Falso // Detiene el ciclo porque el jugador fue derrotado
		FinSi
		
		// Si el juego sigue activo, el jugador ataca al villano
		Si juegoActivo Entonces
			// Elegir una opción de ataque
			Escribir "Quirrel:",vidaQuirrel
			
			Quirrel()
			
			Escribir "Harry Potter:",vidaHP
			Escribir "@-----------------------@"
			Escribir "|1-Ataques              |"
			Escribir "|2-Especial             |"
			Escribir "@-----------------------@"
			leer opc1
			Esperar Tecla
			Limpiar Pantalla
			
			si opc1=2 Entonces
				si vidaHP<50 Entonces
					Escribir "Quirrel: ",vidaQuirrel
					Quirrel()
					
					Escribir "Harry Potter: ",vidaHP
					Escribir "@-----------------------@"
					Escribir "|1-Manos                |"
					Escribir "@-----------------------@"
					leer especial
					si especial=1 Entonces
						Escribir "Quirrel: ",vidaQuirrel
						especialataq=vidaQuirrel
						vidaQuirrel = vidaQuirrel - especialataq
						
						Quirrel()
						
						Escribir "Harry Potter: ",vidaHP
						
						
					FinSi
				sino 
					Escribir "error cuentas con la vida suficiente para la victoria "
					Escribir "no te rindas, asqueroso Traidor a la sangre"
					Esperar Tecla
					Limpiar Pantalla
					
				FinSi
			FinSi
			Escribir "Quirrel: ",vidaQuirrel
			Quirrel()
			
			Escribir "Harry Potter: ",vidaHP
			Escribir "@-----------------------@"
			Escribir "|1-EXPELLIARMUS         |"
			Escribir "|2-WINGARDIUM LEVIOSA   |"
			Escribir "|3-LUMOS                |"
			Escribir "@-----------------------@"
			Leer opcionAtaque
			Esperar Tecla
			Limpiar Pantalla
			// Validar la opción de ataque
			Si opcionAtaque <= 1 O opcionataque=2 o opcionAtaque >= 3 Entonces
				// El daño se toma del vector de ataques según la opción elegida
				Escribir "Quirrel: ",vidaQuirrel
				Quirrel()
				
				Escribir "Harry Potter: ",vidaHP
				Esperar Tecla
				Limpiar Pantalla
				vidaQuirrel = vidaQuirrel - ataques[opcionAtaque]
				Escribir "Quirrel: ",vidaQuirrel
				Quirrelherido()
				
				Escribir "Harry Potter: ",vidaHP
				Escribir "@---------------------------------------------------@"
				Escribir "atacas y causas ", ataques[opcionAtaque], " de daño.|"
				Escribir "|                                                   |"
