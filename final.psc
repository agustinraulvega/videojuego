	Definir vidaRW Como Entero
	Definir vidadraco Como Entero
	Definir dañodraco Como Entero
	Definir opcionAtaque Como Entero
	Definir juegoActivo Como Logico
	
	// Inicializar variables
	vidaRW = 6256
	vidadraco = 6800
	juegoActivo = Verdadero
	dañodraco = azar(1632) 
	// Definir el vector con diferentes opciones de ataque
	Dimension ataques[1292]
	ataques[1] = 1292
	ataques[2] = 646  
	ataques[3] = 323  
	
